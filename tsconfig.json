{
    "include": [   // 자바스크립트로 컴파일 하려는 디렉터리들 추가
        "src"
    ],
    "compilerOptions": {
        "outDir": "build", // 자바스크립트 파일이 생성될 디렉터리 지정
        "target": "ES6", // 자바스크립트 버전 지정
        "lib": ["ES6"], // 타입스크립트에게 어떤 API를 사용하고 어떤 환경에서 코드를 실행하는 지를 지정할 수 있습니다
        "module": "CommonJS",
        "esModuleInterop": true,
        "strict": true,
    }
}   